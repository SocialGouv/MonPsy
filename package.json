  },
  "dependencies": {
    "@dataesr/react-dsfr": "0.9.9",
    "@faker-js/faker": "^6.0.0-alpha.6",
    "@sentry/nextjs": "^6.17.7",
    "@socialgouv/bootstrap.core": "^0.2.1",
    "@socialgouv/matomo-next": "^1.2.2",
    "@types/sequelize": "^4.28.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@zeit/next-source-maps": "^0.0.3",
    "axios": "^0.26.0",
    "bcryptjs": "^2.4.3",
    "cron": "^1.8.2",
    "dotenv": "^11.0.0",
    "got": "12.0.1",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "helmet": "^5.0.2",
    "jest-dev-server": "^6.0.3",
    "joi": "^17.6.0",
    "leaflet": "^1.7.1",
    "next": "12.0.7",
    "next-auth": "^4.2.1",
    "nodemailer": "^6.7.2",
    "p-limit": "3.1.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.2.5",
    "sanitize-html": "^2.7.0",
    "sequelize": "^6.16.1",
