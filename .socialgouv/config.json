{
  "name": "app",
  "type": "app",
  "subdomain": "monpsy",
  "registry": "ghcr",
  "project": "mon-psy-sante",
  "pg": true,
  "devInitContainerCommand": ["yarn", "db:init"],
  "resources": {
    "requests": {
      "cpu": "0.5",
      "memory": "512Mi"
    },
    "limits": {
      "cpu": "1",
      "memory": "1024Mi"
    }
  },
  "probes": {
    "livenessProbe": {
      "timeoutSeconds": 10
    },
    "readinessProbe": {
      "timeoutSeconds": 10
    },
    "startupProbe": {
      "timeoutSeconds": 10
    }
  }
}
